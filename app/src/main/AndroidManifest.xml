<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.kingauthur.recyclerviewfeature"
          xmlns:android="http://schemas.android.com/apk/res/android">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".RVMainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".recyclerview.RVRvMainActivity"/>
    <activity android:name=".recyclerview.RVLinearRvActivity"/>
    <activity android:name=".recyclerview.RVGridRvActivity"/>
    <activity android:name=".recyclerview.RVStaggeredRvActivity"/>
  </application>

</manifest>
